# Ignore directories generated by Composer
drush/contrib
vendor

# Ignore everything in webroot.
web/*

# Only ignore contrib modules in webroot.
!web/modules
web/modules/README.txt
web/modules/contrib

# Only ignore contrib themes in webroot.
!web/themes
web/themes/README.txt
web/themes/contrib

# Only ignore contrib profiles in webroot.
!web/profiles
web/profiles/README.txt
web/profiles/contrib

# Only ignore core files in sites folder of webroot.
!web/sites
web/sites/README.txt
web/sites/example.settings.local.php
web/sites/example.sites.php
web/sites/development.services.yml

# Only ignore core files in sites/default folder.
!web/sites/default
web/sites/default/default*

# Ignore all the user files.
web/sites/*/files/*

# Ignore files generated by PhpStorm
.idea
