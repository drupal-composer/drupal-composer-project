{
    "name": "thinkshout/tt",
    "description": "ThinkShout's project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ThinkShout",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/DrupalDriver.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/drupalextension.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/media.git"
        },
        {
            "type": "git",
            "url": "https://github.com/thinkshout/behat-drush-endpoint.git"
        },
        {
            "type": "package",
            "package": {
                "name": "chosen/chosen",
                "version": "1.6.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "chosen/chosen": "^1.6.1",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/address": "^1.0-rc4",
        "drupal/admin_toolbar": "~1.17",
        "drupal/block_visibility_groups": "^1.1",
        "drupal/chosen": "~2.1",
        "drupal/config_installer": "1.x-dev#128ab9096391ba7d078a6e1c180d613c6bc1176b",
        "drupal/contact_storage": "^1.0-beta8",
        "drupal/core": "~8.3.0",
        "drupal/ctools": "^3.0-alpha27",
        "drupal/devel": "^1.0-beta1",
        "drupal/ds": "2.x-dev",
        "drupal/editor_advanced_link": "^1.3",
        "drupal/email_registration": "^1.0-rc3",
        "drupal/external_entities": "1.x-dev",
        "drupal/fast_404": "^1.0-alpha2",
        "drupal/field_group": "1.x-dev",
        "drupal/flag": "4.x-dev",
        "drupal/google_analytics": "~2.1",
        "drupal/honeypot": "~1.23",
        "drupal/inline_entity_form": "~1.0",
        "drupal/layout_plugin": "^1.0-alpha23",
        "drupal/linkit": "~4.1",
        "drupal/mailchimp": "^1.2",
        "drupal/memcache": "^2.0-alpha2",
        "drupal/menu_block": "~1.4",
        "drupal/metatag": "~1.0",
        "drupal/migrate_google_sheets": "1.x-dev",
        "drupal/migrate_plus": "^3.0-beta1",
        "drupal/migrate_tools": "^3.0-beta1",
        "drupal/media_entity_audio": "1.x-dev#61dedc218a9a023699499b73d6199a3280110e06",
        "drupal/paragraphs": "~1.0",
        "drupal/pathauto": "^1.0-beta1",
        "drupal/r4032login": "1.x-dev",
        "drupal/redirect": "^1.0-alpha2",
        "drupal/redis": "^1.0-beta1",
        "drupal/search_api": "1.x-dev",
        "drupal/search_api_pantheon": "^1.0-alpha2",
        "drupal/search_api_solr": "^1.0-beta1",
        "drupal/slick": "^1.0-rc2",
        "drupal/simple_sitemap": "^2.7",
        "drupal/token": "^1.0-beta2",
        "drush/drush": "^8.1.10",
        "thinkshout/media": "dev-8.x-1.x",
        "thinkshout/robo-drupal": "dev-master",
        "vlucas/phpdotenv": "^2.2",
        "drupal/rrssb": "^2.0-alpha2"
    },
    "require-dev": {
        "drupal/drupal-driver": "dev-master",
        "drupal/drupal-extension": "dev-master",
        "drush-ops/behat-drush-endpoint": "dev-master",
        "jcalderonzumba/gastonjs": "1.1.x-dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Fix views to correctly use IN queries that have an array of values": "https://www.drupal.org/files/issues/2744069-5-views-in-queries.patch",
                "ImageWidget::validateRequiredFields() produces a PHP Warning message if triggering element is a non-button": "https://www.drupal.org/files/issues/2745491-15.patch",
                "DrupalBehaviorError: attach ; quickedit: entityElement.get(...) is undefined https://www.drupal.org/node/2314185": "https://www.drupal.org/files/issues/drupalbehaviorerror-2314185-18.patch",
                "Migration bug: Error when a process plugin gives a multiple value for a subproperty destination": "https://www.drupal.org/files/issues/2789125-subproperties-with-multiple-process-plugin-5.patch",
                "Menu subtrees in menu blocks show all subitems regardless of the active menu item": "https://www.drupal.org/files/issues/drupal-n2631468-95.patch"
            },
            "drupal/config_installer": {
                "Fix issue installing with config_installer profile": "https://www.drupal.org/files/issues/config_installer_2729243_2.patch"
            },
            "drupal/ctools": {
                "Views exposed filters missing autosubmit option https://www.drupal.org/node/2475595": "https://www.drupal.org/files/issues/2475595-ctools-autocomplete-fix-24.patch"
            },
            "drupal/menu_block": {
                "Allow menu blocks to follow active trail and set current page as top-level menu item + Menu parent as block title. Combines patches from http://drupal.org/node/2809699 and http://drupal.org/node/2756675.": "https://gist.githubusercontent.com/caxy4/bf93bc12fc88a08241d5a9d674ddae71/raw/38e822ece35b104bedaf02d2291e712c3c4a0643/menu_block-2756675_and_2809699.patch"
            }
        },
        "drupal-scaffold": {
            "excludes": [
                ".htaccess"
            ]
        }
    }
}